{
	"processOffering": {
		"process": {
			"identifier": "stack_creation",
			"title": "Application StackCreation dynamically added by POST /processes"
		}
	},
	"deploymentProfile": {
		"deploymentProfileName": "deploy_post_workflow",
		"executionUnit": {
			"package": {
			    "cwlVersion": "v1.0",
			    "class": "CommandLineTool",
			    "hints": {
			        "DockerRequirement": {
			            "dockerPull": "docker-registry.crim.ca/ogc-public/debian8-snap6-gpt:v1"
			        }
			    },
			    "inputs": {
			        "source_graph": {
			            "inputBinding": {
			                "position": 1
			            },
			            "type": "File"
			        },
                    "files": {
                        "inputBinding": {
                            "position": 2,
                            "prefix": "-Pfiles=",
                            "separate": false,
                            "itemSeparator": ","
                        },
                        "type": {
                            "type": "array",
                            "items": "File"
                        }
                    },
			        "file_type": {
			            "inputBinding": {
			                "position": 3,
			                "prefix": "-f"
			            },
			            "type": "string"
			        },
			        "output_name": {
			            "inputBinding": {
			                "position": 4,
			                "prefix": "-t"
			            },
			            "type": "string"
			        }
			    },
			    "outputs": {
			        "output": {
			            "outputBinding": {
			                "glob": "$(inputs.output_name)"
			            },
			            "type": "File"
			        }
			    }
			}
		}
	}
}
