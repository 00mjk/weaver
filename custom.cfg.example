[buildout]
extends = buildout.cfg
parts += adapter_impl

## Development options
# versions = noversions

[settings]
configuration = default
hostname = localhost
https-port = 5000
log-level = WARN
wps = true
wps-path = /ows/wps
ows-proxy-protected-path = /ows/proxy
wps-restapi = true
wps-restapi-path = /
adapter = package.path.to.AdapterImpl

extra-options = adapter_impl.opt1=value1,adapter_impl.opt2=value2

[adapter_impl]
recipe = git-recipe
repository = git://github.com/provider/adapter_impl.git
ref = master
as_egg = True
