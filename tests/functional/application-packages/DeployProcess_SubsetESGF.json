                    "time_end": {
                        "type": "float"
                    },
                    "time_crs": {
                        "type": "string"
                    },
                    "lat_start": {
                        "type": "float"
                    },
                    "lat_end": {
                        "type": "float"
                    },
                    "lat_crs": {
                        "type": "string"
                    },
                    "lon_start": {
                        "type": "float"
                    },
                    "lon_end": {
                        "type": "float"
                    },
                    "lon_crs": {
                        "type": "string"
                    },
                    "api_key": {
                        "type": "string"
                    }
                },
                "outputs": {
                    "output_netcdf": {
